{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "Addapptables boilerplate",
  "author": "Addapptables",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint . --ext .ts",
    "test": "mocha -r 'test/index.spec.js' test/**/*.spec.ts --exit",
    "test-ci": "yarn docker-down && yarn docker-build && yarn docker-run && yarn docker-down",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ./ormconfig.ts",
    "docker-dev": "docker-compose up postgresql",
    "coverage": "nyc yarn test",
    "coverage-report": "nyc --reporter=lcov yarn test"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@addapptables/microservice": "1.1.0",
    "@nestjs/common": "^7.2.0",
    "@nestjs/core": "^7.2.0",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.2.0",
    "@nestjs/swagger": "4.5.11",
    "class-transformer": "^0.2.3",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.12.2",
    "connect-redis": "^4.0.4",
    "cookie-parser": "^1.4.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.3",
    "express-session": "^1.17.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "ramda": "^0.27.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "sqlite3": "^5.0.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "using-statement": "^0.3.1",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^7.2.0",
    "@types/chai": "4.2.11",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.14",
    "@types/ramda": "^0.27.6",
    "@types/redis": "^2.8.22",
    "@types/sinon": "^9.0.4",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.8.1",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2",
    "ts-loader": "^7.0.5",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  }
}
